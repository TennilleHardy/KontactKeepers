<?xml version='1.0'?>
<package xmlns='http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd'>
    <metadata>
        <id>Facebook.Client</id>
        <version>$version$</version>
        <title/>
        <authors>Outercurve Foundation</authors>
        <owners>Sanjeev Dwivedi, Prabir Shrestha</owners>
        <description><![CDATA[The Facebook SDK for Windows & Windows Phone helps .Net developers build phone and Windows Store applications that integrate with Facebook.]]></description>
        <language>en-US</language>
        <licenseUrl>https://github.com/facebook-csharp-sdk/facebook-winclient-sdk/master/LICENSE.txt</licenseUrl>
        <projectUrl>https://github.com/facebook-csharp-sdk/facebook-winclient-sdk</projectUrl>
        <iconUrl>http://csharpsdk.org/assets/img/logo75x75.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <tags>Facebook fb</tags>
        <dependencies>
            <group>
                <dependency id="Facebook" version="6.4" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="..\..\..\LICENSE.txt" target = "LICENSE.txt" />
        <file src="..\..\..\Build\readme.txt" target = "readme.txt" />
        
        <file src="..\..\..\Bin\Release\wp8\Facebook.Client.dll" target = "lib\wp8" />
        <file src="..\..\..\Bin\Release\wp8\Facebook.Client.xml" target = "lib\wp8" />
        <file src="..\..\..\Bin\Release\wp8\Facebook.Client.pdb" target = "lib\wp8" />

        <file src="..\..\..\Bin\Release\win8\Facebook.Client.dll" target = "lib\win8" />
        <file src="..\..\..\Bin\Release\win8\Facebook.Client.pdb" target = "lib\win8" />
        <file src="..\..\..\Bin\Release\win8\Facebook.Client.xml" target = "lib\win8" />

        <!-- Windows Phone 8.1+ -->
        <file src="..\..\..\Bin\Release\WindowsPhoneUniversal\Facebook.Client.dll" target = "lib\wpa81" />
        <file src="..\..\..\Bin\Release\WindowsPhoneUniversal\Facebook.Client.pdb" target = "lib\wpa81" />
        <file src="..\..\..\Bin\Release\WindowsPhoneUniversal\Facebook.Client.xml" target = "lib\wpa81" />

        <!-- Windows 8.1+ -->
        <file src="..\..\..\Bin\Release\WindowsUniversal\Facebook.Client.dll" target = "lib\win81" />
        <file src="..\..\..\Bin\Release\WindowsUniversal\Facebook.Client.pdb" target = "lib\win81" />
        <file src="..\..\..\Bin\Release\WindowsUniversal\Facebook.Client.xml" target = "lib\win81" />
    </files>
</package>
